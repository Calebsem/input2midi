{
  "name": "input2midi",
  "version": "1.0.0",
  "description": "mouse, keyboard and gamepads as midi outputs on macosx",
  "main": "index.js",
  "author": "Calebsem <penguzine@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "debug": "node --inspect-brk=9229 index.js"
  },
  "dependencies": {
    "inquirer": "^6.0.0",
    "iohook": "^0.2.0",
    "midi": "^0.9.5",
    "module-alias": "^2.1.0",
    "screenres": "^2.0.1"
  },
  "_moduleAliases": {
    "@root": ".",
    "@app": "src",
    "@lib": "src/lib",
    "@events": "src/lib/events"
  },
  "iohook": {
    "targets": [
      "node-57"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  }
}
